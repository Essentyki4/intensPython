#include <iostream>
using namespace std;

int main()
{
    int x1, y1, x2, y2;
    cout << "Введите координаты вектора A (x1, y1): ";
    cin >> x1 >> y1;
    cout << "Введите координаты вектора B (x2, y2): ";
    cin >> x2 >> y2;
    cout << "Векторное произведение: " << (x1 * y2 - x2 * y1) << endl;
    
}
