#include <stdio.h>

int main() {
    int N, K;

    // Ввод размера массива N и числа K
    printf("Введите размер массива N: ");
    scanf("%d", &N);
    
    if (N <= 0) {
        printf("Размер массива должен быть положительным.\n");
        return 1;
    }

    printf("Введите значение K: ");
    scanf("%d", &K);
    
    if (K <= 0 || K > N) {
        printf("K должно быть в диапазоне от 1 до N.\n");
        return 1;
    }

    int A[N];
    
    // Ввод элементов массива
    printf("Введите элементы массива A:\n");
    for (int i = 0; i < N; i++) {
        scanf("%d", &A[i]);
    }

    // Вывод элементов массива с порядковыми номерами, кратными K
    printf("Элементы массива с порядковыми номерами, кратными %d:\n", K);
    for (int i = K - 1; i < N; i += K) {
        printf("%d ", A[i]);
    }
    printf("\n");

    return 0;
}